"VertexLitGeneric"
{
	$basetexture "models/coduo/skins/propeller_a_b17_prop"
	$translucent 1
	$nocull 1
	
	"$angle" 0.0
	"$translate" "[0.0 0.0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"LinearRamp"
		{
			"rate" -1440
			"initialValue" 0.0
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}